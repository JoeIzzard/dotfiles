# Path to your oh-my-zsh installation.
export ZSH="/home/joe/.oh-my-zsh"

export PATH="/usr/bin:/bin:/usr/sbin:/sbin:/home/monitor/LX-Monitor/Bin:~/dotfiles/Lib/Submodules/Useful-Scripts/Bin:$PATH"

# Disable EOL Mark
export PROMPT_EOL_MARK=''

# Load dotfiles
source ~/.dotfiles/.load

# ZSH_Config
ZSH_THEME="agnoster"

# Plugins
plugins=(git)

# Load Oh My ZSH
source $ZSH/oh-my-zsh.sh